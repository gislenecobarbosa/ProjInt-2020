    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.PrimeiraTelaActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AutenticacaoActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.EmpresaActivity" />
        <activity
            android:name=".activity.ConfEmpresaActivity"
            android:parentActivityName=".activity.EmpresaActivity" />
        <activity
            android:name=".activity.NovoProdutoActivity"
            android:parentActivityName=".activity.EmpresaActivity" />
        <activity
            android:name=".activity.ConfUsuarioActivity"
            android:parentActivityName=".activity.HomeActivity" />
        <activity
            android:name=".activity.CatalogoActivity"
            android:parentActivityName=".activity.HomeActivity" />
        <activity android:name=".activity.PedidosActivity"
            android:parentActivityName=".activity.EmpresaActivity"></activity>

    </application>

</manifest>